{
  "@context": "https://schema.org",
  "@type": "BreadcrumbList",
  "itemListElement": [
    {
      "@type": "ListItem",
      "position": 1,
      "name": "{{ button_home }}",
      "item": "https://withframes.com{% if lang != 'en' %}/{{ lang }}{% endif %}/"
    },
    {
      "@type": "ListItem",
      "position": 2,
      "name": "{{ blog_heading }}",
      "item": "https://withframes.com{% if lang != 'en' %}/{{ lang }}{% endif %}/blog/"
    },
    {
      "@type": "ListItem",
      "position": 3,
      "name": "{{ category_name }}",
      "item": "{{ canonical_url }}"
    }
  ]
}
