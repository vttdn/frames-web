{
  "@context": "https://schema.org",
  "@type": "Blog",
  "@id": "{{ canonical_url }}",
  "url": "{{ canonical_url }}",
  "name": "{{ page_title }}",
  "description": "{{ page_description }}",
  "inLanguage": "{{ lang }}",
  "publisher": {
    "@id": "https://withframes.com/#organization"
  },
  "blogPost": [
    {% for entry in entries %}
    {
      "@type": "BlogPosting",
      "@id": "https://withframes.com{% if lang != 'en' %}/{{ lang }}{% endif %}/changelog/{{ entry.url_slug }}/",
      "url": "https://withframes.com{% if lang != 'en' %}/{{ lang }}{% endif %}/changelog/{{ entry.url_slug }}/",
      "headline": "{{ entry.title }}",
      "description": "{% if entry.meta_description %}{{ entry.meta_description }}{% else %}{{ entry.summary }}{% endif %}",
      "datePublished": "{{ entry.release_date }}",
      "dateModified": "{{ build_date }}",
      "inLanguage": "{{ lang }}",
      "author": {"@id": "https://withframes.com/#vincent-tantardini"},
      "publisher": {
        "@id": "https://withframes.com/#organization"
      }{% if entry.og_image_url and entry.og_image_url != '/og-image.jpg' %},
      "image": {
        "@type": "ImageObject",
        "url": "https://withframes.com{{ entry.og_image_url }}",
        "contentUrl": "https://withframes.com{{ entry.og_image_url }}",
        "dateCreated": "{{ entry.release_date }}",
        "caption": "{{ entry.og_image_alt }}"
      }{% endif %}
    }{% if not loop.last %},{% endif %}
    {% endfor %}
  ]
}
