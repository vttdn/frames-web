{
  "@context": "https://schema.org",
  "@type": "BreadcrumbList",
  "itemListElement": [
    {
      "@type": "ListItem",
      "position": 1,
      "name": "Home",
      "item": "https://withframes.com{% if lang != 'en' %}/{{ lang }}{% endif %}/"
    },
    {
      "@type": "ListItem",
      "position": 2,
      "name": "{{ changelog_heading }}",
      "item": "https://withframes.com{% if lang != 'en' %}/{{ lang }}{% endif %}/changelog/"
    }{% if entry_title %},
    {
      "@type": "ListItem",
      "position": 3,
      "name": "{{ entry_title }}",
      "item": "{{ canonical_url }}"
    }{% endif %}
  ]
}
